<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="entity.Sign_info" table="sign_info">
  <id name="signment_id"  type="java.lang.Integer">
   <column name="signment_id"/>
   <generator class="identity"/>
  </id>
  <property name="student_id" type="java.lang.Integer">
  	<column name="student_id">
  		<comment>学号</comment>
  	</column>
  </property>
  <property name="department_id" type="java.lang.Integer">
  	<column name="department_id">
  		<comment>部门ID</comment>
  	</column>
  </property>
  <property name="personal_intro" type="java.lang.String">
  	<column name="personal_intro">
  		<comment>个人介绍</comment>
  	</column>
  </property>
  <property name="adjust" type="java.lang.Integer">
  	<column name="adjust">
  		<comment>是狗调剂</comment>
  	</column>
  </property>
  <property name="check_state" type="java.lang.Integer">
  	<column name="check_state">
  		<comment>审核状态</comment>
  	</column>
  </property>
  <property name="other" type="java.lang.Integer">
  	<column name="other">
  		<comment>被调剂部门</comment>
  	</column>
  </property>
  <property name="date" type="java.sql.Date">
  	<column name="date">
  		<comment>记录生成日期</comment>
  	</column>
  </property>
  <property name="sys_date" type="java.sql.Date">
  	<column name="sys_date">
  		<comment>记录更新日期</comment>
  	</column>
  </property>
    
 </class>


</hibernate-mapping>
