  	</column>
  </property>  
  <property name="def1" type="java.lang.String">
  	<column name="def1">
  		<comment>学号</comment>
  	</column>
  </property>
  <property name="def2" type="java.lang.String">
  	<column name="def2">
  		<comment>学号</comment>
  	</column>
  </property> 
  <property name="def3" type="java.lang.String">
  	<column name="def3">
  		<comment>学号</comment>
  	</column>
  </property>
</class>

</hibernate-mapping>
