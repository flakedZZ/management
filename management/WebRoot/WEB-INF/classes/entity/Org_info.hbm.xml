<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="entity.Org_info" table="org_info">
  <id name="org_id" type="java.lang.Integer">
   <column name="org_id"/>
   <generator class="identity"/>
  </id>
  <property generated="never" lazy="false" name="org_name" type="java.lang.String">
   <column name="org_name"/>
  </property>
  <property column="org_admin" name="org_admin" type="java.lang.String"/>
  <property column="org_intro" name="org_intro" type="java.lang.String"/>
  <property column="org_qq" name="org_qq" type="java.lang.Integer"/>
  <property column="org_email" name="org_email" type="java.lang.String"/>
  <property column="org_email_username" name="org_email_username" type="java.lang.String"/>
  <property column="org_email_password" name="org_email_password" type="java.lang.String"/>
  <property column="date" name="date" type="java.sql.Date"/>
  <property column="sys_date" name="sys_date" type="java.sql.Date"/>
  <property column="finish_date" name="finish_date" type="java.sql.Date"/>
  <property column="info_publisher" name="info_publisher" type="java.lang.String"/>
  <property column="flag" name="flag" type="java.lang.Integer"/>
  <property column="def1" name="def1" type="java.lang.String"/>
  <property column="def2" name="def2" type="java.lang.String"/>
  <property column="def3" name="def3" type="java.lang.String"/>
  
 </class>
</hibernate-mapping>
