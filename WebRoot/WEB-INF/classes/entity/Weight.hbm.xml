<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="entity.Weight" table="weight">
		<id name="weight_id" type="java.lang.Integer">
			<column name="weight_id"/>
			<generator class="identity"/>
		</id>
		<property name="weight_name" type="java.lang.String">
			<column name="weight_name">
				<comment>用户名</comment>
			</column>
		</property>	
		<property name="flag" type="java.lang.Integer">
			<column name="flag">
				<comment>软删除标记</comment>
			</column>
		</property>
		<property name="date" type="java.sql.Date">
			<column name="date">
				<comment>注册日期</comment>
			</column>
		</property>
		<property name="sysdate" type="java.sql.Date">
			<column name="sys_date">
				<comment>时间戳</comment>
			</column>
		</property>
	
	</class>




</hibernate-mapping>
