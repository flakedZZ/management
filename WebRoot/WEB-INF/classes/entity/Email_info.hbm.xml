<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="entity.Email_info" table="email_info">
  <id name="email_id" type="java.lang.Integer">
   <column name="email_id"/>
   <generator class="identity"/>
  </id>	
   <property name="department_id" type="java.lang.Integer">
  	<column name="department_id">
  		<comment>部门编号</comment>
  	</column>
  </property>  
  <property name="mail_title" type="java.lang.String">
  	<column name="mail_title">
  		<comment>邮件标题</comment>
  	</column>
  </property>
  <property name="mail_text" type="java.lang.String">
  	<column name="mail_text">
  		<comment>邮件内容</comment>
  	</column>
  </property>
  <property name="sender" type="java.lang.String">
  	<column name="sender">
  		<comment>发送者</comment>
  	</column>
  </property>  
  <property name="check_state" type="java.lang.Integer">
  	<column name="check_state">
  		<comment>学号</comment>
  	</column>
  </property>  
  <property name="department_qqgroup" type="java.lang.Integer">
  	<column name="department_qqgroup">
  		<comment>学号</comment>
  	</column>
  </property>  
  <property name="date" type="java.util.Date">
  	<column name="date">
  		<comment>学号</comment>
  	</column>
  </property>  
  <property name="sys_date" type="java.util.Date">
  	<column name="sys_date">
  		<comment>学号</comment>
  	</column>
  </property>  
  
  <property name="flag" type="java.lang.Integer">
  	<column name="flag">
  		<comment>学号</comment>
  	</column>
  </property>
  <property name="def1" type="java.lang.String">
  	<column name="def1">
  		<comment>学号</comment>
  	</column>
  </property>
  <property name="def2" type="java.lang.String">
  	<column name="def2">
  		<comment>学号</comment>
  	</column>
  </property> 
  <property name="def3" type="java.lang.String">
  	<column name="def3">
  		<comment>学号</comment>
  	</column>
  </property>
</class>

</hibernate-mapping>
